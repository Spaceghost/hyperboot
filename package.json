{
  "name": "hyperboot",
  "version": "2.1.0",
  "bin": {
    "hyperboot": "bin/cmd.js"
  },
  "description": "offline webapp bootloader",
  "main": "index.js",
  "browser": "browser/index.js",
  "dependencies": {
    "class-list": "~0.1.1",
    "concat-stream": "~1.4.6",
    "defined": "~0.0.0",
    "frame-rpc": "^1.1.0",
    "has": "~1.0.0",
    "hyperglue": "~1.3.3",
    "hyperstream": "~1.2.1",
    "inherits": "~2.0.1",
    "isarray": "~0.0.1",
    "minimist": "~1.1.0",
    "mkdirp": "~0.5.0",
    "osenv": "~0.1.0",
    "parents": "~1.0.0",
    "route-prefix": "~1.0.1",
    "routes": "~1.2.0",
    "semver-compare": "^1.0.0",
    "sha256": "~0.1.1",
    "through2": "~1.1.1",
    "xhr": "~1.17.0"
  },
  "devDependencies": {
    "html-inline": "^1.1.0",
    "onchange": "~0.0.1",
    "tape": "^3.0.0",
    "uglify-js": "^2.4.15"
  },
  "scripts": {
    "test": "tape test/*.js",
    "build": "browserify browser/preboot.js | uglifyjs -cm > static/hyperboot.js",
    "watch-js": "watchify browser/preboot.js -o static/hyperboot.js -v",
    "watch": "npm run watch-js & onchange static/hyperboot.js static/hyperboot.css static/site.html -- npm run inline",
    "inline": "html-inline static/site.html > static/index.html",
    "prepublish": "npm run build && npm run inline"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/substack/hyperboot.git"
  },
  "homepage": "https://github.com/substack/hyperboot",
  "keywords": [
    "offline",
    "webapp",
    "versions",
    "boot",
    "applicationCache",
    "cache",
    "offline-first"
  ],
  "author": {
    "name": "James Halliday",
    "email": "mail@substack.net",
    "url": "http://substack.net"
  },
  "license": "MIT"
}
